{
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
        "no-console": "warn",
        "quotes": ["warn", "double"],
        "prefer-const": "warn",
        "jsx-quotes": ["warn", "prefer-double"],
        "indent": ["warn", 2],
        "max-len": ["warn", {"code": 120}],
        "comma-dangle": ["warn", "always-multiline"],
        "semi": ["warn", "always"]
    }
}